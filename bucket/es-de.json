    }
  },
  "bin": "ES-DE.exe",
  "shortcuts": [
    ["ES-DE.exe", "ES-DE"]
  ],
  "persist": "ES-DE",
  "pre_install": [
    "if (!(Test-Path \"$dir\\ES-DE\")) { New-Item \"$dir\\ES-DE\" -ItemType Directory | Out-Null }"
  ],
  "checkver": {
    "url": "https://gitlab.com/es-de/emulationstation-de/-/raw/master/latest_release.json",
    "jp": "$.stable.version"
  },
  "autoupdate": {
    "architecture": {
      "64bit": {
        "url": "$($info[\"stable\"][\"WindowsPortable\"][\"url\"])",
        "hash": "md5:$($info[\"stable\"][\"WindowsPortable\"][\"md5\"])"
      }
    },
    "script": [
      "$json = Invoke-RestMethod 'https://gitlab.com/es-de/emulationstation-de/-/raw/master/latest_release.json'",
      "$version = $json.stable.version",
      "$url = $json.stable.WindowsPortable.url",
      "$hash = 'md5:' + $json.stable.WindowsPortable.md5",
      "Write-Output (\"version=$version`nurl=$url`nhash=$hash\")"
    ]
  }
}
